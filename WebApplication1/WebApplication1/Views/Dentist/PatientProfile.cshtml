@model WebApplication1.ViewModels.PatientProfileViewModel
@using WebApplication1.Enumerations
@* @model WebApplication1.Models.MedicalRecord *@
@{
    ViewBag.Title = "PatientProfile";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<head>
    <link href="~/Content/css/CustomLeftNavbar.css" rel="stylesheet" />
    <link href="~/Content/css/CustomProfile.css" rel="stylesheet" />
    <link href="~/Content/css/TeethImages.css" rel="stylesheet" />
</head>


<div class="container my-custom-container">
    <div class="profile-head">
        <div class="col-md- col-sm-4 col-xs-12">
            <img src="http://www.newlifefamilychiropractic.net/wp-content/uploads/2014/07/300x300.gif" class="img-responsive" />
            <h6>
                @Html.DisplayFor(d => d.Patient.FirstName)
                @Html.DisplayFor(d => d.Patient.LastName)
            </h6>
        </div>
        <div class="col-md-5 col-sm-5 col-xs-12">
            <h5>@Html.DisplayFor(d => d.Patient.FirstName) @Html.DisplayFor(d => d.Patient.LastName) </h5>
            <p>Patient </p>
            <ul>
                <li><span class="glyphicon glyphicon-gift"></span>@Html.DisplayFor(model => model.Patient.DateOfBirth)</li>
                <li><span class="glyphicon glyphicon-home"></span>@Html.DisplayFor(model => model.Patient.Address) </li>
                <li><span class="glyphicon glyphicon-phone"></span> <a href="#" title="call">@Html.DisplayFor(model => model.Patient.PhoneNumber) </a></li>
                <li><span class="glyphicon glyphicon-envelope"></span><a href="#" title="mail">@Html.DisplayFor(model => model.Patient.Email)</a></li>
            </ul>
        </div>

    </div>
</div>

<div class="container">
    <div class="row">
        <div class="col-sm-3 col-md-3">
            <div class="panel-group" id="accordion">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h4 class="panel-title">
                            <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne">
                                <span class="glyphicon glyphicon-folder-close">
                                </span>Patient's Medical Records
                            </a>
                        </h4>
                    </div>
                    <div id="collapseOne" class="panel-collapse collapse">
                        <div class="panel-body">
                            <table class="table">
                                <td>
                                    <div class="form-horizontal" ">

                                        <ul>
                                            @foreach (var item in Model.MedicalRecords)
                                            {
                                                
                                                <span style="color:black; font-family: ‘Palatino Linotype’, ‘Book Antiqua’, Palatino, serif; font-size:large;"> Date: @Html.DisplayFor(modelItem => item.DateCreated).ToHtmlString()</span>
                                                <br>
                                                <br>
                                                <span style="color: black; font-size:large; font-family: ‘Palatino Linotype’, ‘Book Antiqua’, Palatino, serif;">Description:</span>                                              
                                                <span style="color: black; font-size: larger; font-family: ‘Palatino Linotype’, ‘Book Antiqua’, Palatino, serif;">@Html.DisplayFor(modelItem => item.Description).ToHtmlString()</span>
                                                <br>
                                                <br>
                                                <span style="color: black; font-size:large; font-family: ‘Palatino Linotype’, ‘Book Antiqua’, Palatino, serif;">Position</span>
                                                <span style="color: black;  font-size:large; float:right; font-family: ‘Palatino Linotype’, ‘Book Antiqua’, Palatino, serif;">State</span>
                                                <br>
                                                <span style="color:black; font-size:larger; font-family: ‘Palatino Linotype’, ‘Book Antiqua’, Palatino, serif;"> @Html.DisplayFor(modelItem => item.ToothPosition)</span>
                                                <span style="float: right; color:black; font-size:larger; font-family: ‘Palatino Linotype’, ‘Book Antiqua’, Palatino, serif;"> @Html.DisplayFor(modelItem => item.ToothState)</span>
                                                <br>
                                                <br>
                                                <span style="float: right; color:blue; font-size:medium;"> @Html.DisplayFor(modelItem => item.Bill)</span>
                                                <hr />

                                            }

                                        </ul>
                                    </div>
                                    @Html.ActionLink("List all medical records", "ListAllMedicalRecords", new { id = Model.Patient.Id }, new { @class = "btn btn-success" })


                                </td>
                                </tr>
                            </table>
                        </div>
                    </div>
                </div>
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h4 class="panel-title">
                            <a data-toggle="collapse" data-parent="#accordion" href="#collapseTwo">
                                <span class="glyphicon glyphicon-folder-close">
                                </span>Add Medical Record
                            </a>
                        </h4>
                    </div>
                    <div id="collapseTwo" class="panel-collapse collapse">
                        <div class="panel-body">
                            <table class="table">
                                <tr>
                                    <td>
                                        <div class="form-horizontal">
                                           
                                            @using (Html.BeginForm(FormMethod.Post))
                                            {
                                                @Html.AntiForgeryToken()

                                                <div class="form-horizontal">                                                
                                                    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                                                    <div class="main">
                                                        @Html.TextBoxFor(m => m.MedicalRecord.Description, new { @class = "form-control", @placeholder = "Description" })
                                                        @Html.EnumDropDownListFor(m => m.Tooth.ToothPosition, new { @class = "form-control", @placeholder = "Tooth Position" })
                                                        @Html.EnumDropDownListFor(m => m.Tooth.ToothState, new { @class = "form-control", @placeholder = "Tooth State" })
                                                        @Html.TextBoxFor(m => m.MedicalRecord.Bill, new { @class = "form-control", @placeholder = "Bill" })

                                                        <span class="clearfix"></span>
                                                    </div>
                                                    <div class="col-md-offset-2 col-md-10" style="padding-top: 10px; ">
                                                        <input type="submit" value="Insert" onclick="location.href='@Url.Action("Create", "InsertMedicalRecord2")'" class="btn btn-primary" />
                                                    </div>
                                                </div>
                                            }
                                        </div>
                                    </td>
                                </tr>
                            </table>
                        </div>
                    </div>
                </div>
