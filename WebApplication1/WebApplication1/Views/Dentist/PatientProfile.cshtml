                    <div id="collapseTwo" class="panel-collapse collapse">
                        <div class="panel-body">
                            <table class="table">
                                <tr>
                                    <td>
                                        <div class="form-horizontal">
                                            <h4>Add new record</h4>
                                            <hr />
                                            @using (Html.BeginForm())
                                            {
                                                @Html.AntiForgeryToken()

                                                <div class="form-horizontal">
                                                    <hr />
                                                    @Html.AntiForgeryToken()
                                                    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                                                    <div class="main">
                                                        @Html.TextBoxFor(m => m.MedicalRecords, new { @class = "form-control", @placeholder = "Date Created" })
                                                        @Html.TextBoxFor(m => m.MedicalRecords, new { @class = "form-control", @placeholder = "Description" })
                                                        @Html.TextBoxFor(m => m.MedicalRecords, new { @class = "form-control", @placeholder = "Bill" })

                                                        <span class="clearfix"></span>
                                                    </div>
                                                    <div class="form-group">
                                                        <div class="col-md-offset-2 col-md-10">
                                                            <input type="submit" value="Create" class="btn btn-default" />
                                                        </div>
                                                    </div>
                                                </div>
                                            }
                                        </div>
                                    </td>
                                </tr>
                            </table>
                        </div>
                    </div>
